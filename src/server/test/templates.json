{
    "version": "2",
    "templates": [
        {
            "title": "Wordpress",
            "description": "WordPress est l'application web la plus utilisé au monde pour créer un beau site web.",
            "categories": [
                "blog"
            ],
            "enabled": true,
            "price": 19,
            "trial": 7,
            "adminPath": "/wp-admin",
            "logo": "https://i.imgur.com/7WXHj55.png",
            "repository": {
                "url": "https://github.com/ethibox/awesome-stacks",
                "stackfile": "stacks/wordpress.yml"
            }
        },
        {
            "title": "Flarum",
            "description": "Flarum est un logiciel de forum nouvelle génération qui rend les discussions en ligne amusantes.",
            "categories": [
                "forum"
            ],
            "enabled": true,
            "price": 19,
            "trial": 7,
            "logo": "https://i.imgur.com/oUlWx8o.png",
            "repository": {
                "url": "https://github.com/ethibox/awesome-stacks",
                "stackfile": "stacks/flarum.yml"
            },
            "env": [
                {
                    "name": "ADMIN_USERNAME",
                    "label": "Admin username",
                    "value": "admin",
                    "disabled": true
                },
                {
                    "name": "ADMIN_EMAIL",
                    "label": "Admin email",
                    "disabled": true
                },
                {
                    "name": "ADMIN_PASSWORD",
                    "label": "Admin password",
                    "type": "password",
                    "disabled": true
                },
                {
                    "name": "FLARUM_TITLE",
                    "label": "Flarum Title"
                }
            ]
        },
        {
            "title": "Invoice-ninja",
            "description": "Invoice Ninja est une application libre et open-source pour la facturation de vos clients.",
            "categories": [
                "billing"
            ],
            "enabled": true,
            "price": 19,
            "trial": 7,
            "logo": "https://i.imgur.com/r6TPGAZ.jpg",
            "repository": {
                "url": "https://github.com/ethibox/awesome-stacks",
                "stackfile": "stacks/invoiceninja.yml"
            },
            "env": [
                {
                    "name": "REQUIRE_HTTPS",
                    "value": "true",
                    "preset": true
                },
                {
                    "name": "HTTPS",
                    "value": "1",
                    "preset": true
                },
                {
                    "name": "APP_KEY",
                    "value": "S0meR@nd0mStringS0meR@nd0mString",
                    "preset": true
                },
                {
                    "name": "SMTP_HOST",
                    "label": "SMTP Host"
                },
                {
                    "name": "SMTP_USERNAME",
                    "label": "SMTP Username"
                },
                {
                    "name": "SMTP_PASSWORD",
                    "label": "SMTP Password",
                    "type": "password"
                },
                {
                    "name": "SMTP_ENCRYPTION",
                    "label": "SMTP Encryption",
                    "type": "select",
                    "select": [
                        {
                            "text": "TLS",
                            "value": "tls"
                        }
                    ]
                },
                {
                    "name": "SMTP_PORT",
                    "label": "SMTP Port",
                    "type": "number"
                },
                {
                    "name": "SMTP_FROM_NAME",
                    "label": "SMTP From Name"
                },
                {
                    "name": "SMTP_FROM_ADDRESS",
                    "label": "SMTP From Address",
                    "type": "email"
                }
            ]
        },
        {
            "title": "Fathom",
            "description": "Fathom",
            "categories": [
                "analytics"
            ],
            "enabled": true,
            "price": 19,
